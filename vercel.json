{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "scripts.js", "use": "@vercel/static" },
    { "src": "sw.js", "use": "@vercel/static" },
    { "src": "manifest.webmanifest", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.(?:css|js|mjs|map|png|jpg|jpeg|gif|webp|svg|ico|mp3|wav|ogg|json|webmanifest|txt|html))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
